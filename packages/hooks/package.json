{
    "name": "@alfalab/hooks",
    "version": "1.6.1",
    "description": "common hooks",
    "sideEffects": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.mjs",
    "files": [
        "dist",
        "*.js",
        "*.mjs",
        "*.d.ts",
        "!rollup.config.js"
    ],
    "scripts": {
        "remove-junk": "node ../../scripts/remove-junk",
        "update-exports": "node ../../scripts/update-exports.js",
        "build": "yarn remove-junk && yarn build:ts && yarn update-exports && yarn build:dts",
        "build:ts": "rollup -c",
        "build:dts": "dts-bundle-generator --no-banner --no-check --config dts.json"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react": "^11.2.7",
        "@testing-library/react-hooks": "^7.0.0",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.5",
        "dts-bundle-generator": "^5.6.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "peerDependencies": {
        "@alfalab/utils": "^1.5.0",
        "react": ">=16.14.0 || >= 17.0.2"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "10dd0711fab88c3173da9cd72abe50194559d021",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./package.json": "./package.json",
        "./useClickOutside": {
            "import": "./dist/useClickOutside/index.mjs",
            "require": "./dist/useClickOutside/index.js"
        },
        "./useCountdown": {
            "import": "./dist/useCountdown/index.mjs",
            "require": "./dist/useCountdown/index.js"
        },
        "./useDidUpdateEffect": {
            "import": "./dist/useDidUpdateEffect/index.mjs",
            "require": "./dist/useDidUpdateEffect/index.js"
        },
        "./useEventCallback": {
            "import": "./dist/useEventCallback/index.mjs",
            "require": "./dist/useEventCallback/index.js"
        },
        "./useFocus": {
            "import": "./dist/useFocus/index.mjs",
            "require": "./dist/useFocus/index.js"
        },
        "./useImageLoadingState": {
            "import": "./dist/useImageLoadingState/index.mjs",
            "require": "./dist/useImageLoadingState/index.js"
        },
        "./useKeydownOutside": {
            "import": "./dist/useKeydownOutside/index.mjs",
