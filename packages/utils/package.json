{
    "name": "@alfalab/utils",
    "version": "1.7.0",
    "description": "common utils",
    "sideEffects": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/esm/index.js",
    "files": [
        "dist",
        "*.js",
        "*.mjs",
        "*.d.ts",
        "!rollup.config.js"
    ],
    "scripts": {
        "remove-junk": "node ../../scripts/remove-junk",
        "update-exports": "node ../../scripts/update-exports.js",
        "build": "yarn remove-junk && yarn build:ts && yarn update-exports && yarn build:dts",
        "build:ts": "rollup -c",
        "build:dts": "dts-bundle-generator --no-banner --config dts.json"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "dts-bundle-generator": "^5.6.0",
        "rollup": "^2.38.0"
    },
    "peerDependencies": {
        "@alfalab/data": "^0.4.5"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "10dd0711fab88c3173da9cd72abe50194559d021",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./package.json": "./package.json",
        "./crop-account-number": {
            "import": "./dist/crop-account-number/index.mjs",
            "require": "./dist/crop-account-number/index.js"
        },
